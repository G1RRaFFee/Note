model User {
    id           Int           @id @default(autoincrement())
    username     String
    email        String        @unique
    password     String
    createdAt    DateTime      @default(now()) @map("created_at")
    updatedAt    DateTime      @updatedAt @map("updated_at")
    refreshToken RefreshToken?
    contacts     Contact[]

    @@map("users")
}
